{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "bilingual_type": "hover over",
      "uhfHeaderId": "MSDocsHeader-AdaptiveCards",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "ms.prod": "adaptive-cards",
      "titleSuffix": "Adaptive Cards",
      "searchScope": ["Adaptive Cards"],
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/AdaptiveCards",
      "disable_contribution_on_translation_quality": ["MTE75","MT"]
    },
    "fileMetadata": {
       "open_to_public_contributors": {
          "getting-started/**.md": true,
          "authoring-cards/**.md": true
        }
    },
    "template": [],
    "dest": "AdaptiveCards",
    "markdownEngineName": "markdig"
  }
}
